class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class Stack:
    def __init__(self):
        self.top = None   # Points to the top node in stack
        self._size = 0    # Track size of the stack

    def is_empty(self):
        """Check if the stack is empty"""
        return self.top is None
        # Time: O(1), Space: O(1)

    def push(self, item):
        """Add item to the top of the stack"""
        new_node = Node(item)
        new_node.next = self.top
        self.top = new_node
        self._size += 1
        # Time: O(1), Space: O(1)

    def pop(self):
        """Remove and return the top item from the stack"""
        if self.is_empty():
            return None
        popped_node = self.top
        self.top = self.top.next
        self._size -= 1
        return popped_node.data
        # Time: O(1), Space: O(1)

    def peek(self):
        """Return the top item without removing it"""
        if self.is_empty():
            return None
        return self.top.data
        # Time: O(1), Space: O(1)

    def size(self):
        """Return the number of elements in the stack"""
        return self._size
        # Time: O(1), Space: O(1)

    def get_all_elements(self):
        """Return a list of all elements from top to bottom"""
        elements = []
        current = self.top
        while current:
            elements.append(current.data)
            current = current.next
        return elements

    def print_stack(self):
        """Print all elements in the stack from top to bottom"""
        elements = self.get_all_elements()
        for elem in elements:
            print(elem)
s = Stack()

s.push(10)
s.push(20)
s.push(30)

print(s.peek())     # Output: 30
print(s.pop())      # Output: 30
print(s.peek())     # Output: 20
print(s.size())     # Output: 2
print(s.is_empty()) # Output: False
